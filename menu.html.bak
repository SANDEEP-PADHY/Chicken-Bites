  <script nomodule src="https://unpkg.com/ionicons@5.5.2/dist/ionicons/ionicons.js"></script>

  <!-- Advanced Animations Script -->
  <script>
    // Smooth scroll reveal animations
    const observerOptions = {
      threshold: 0.1,
      rootMargin: '0px 0px -100px 0px'
    };

    const fadeInObserver = new IntersectionObserver((entries) => {
      entries.forEach(entry => {
        if (entry.isIntersecting) {
          entry.target.style.opacity = '1';
          entry.target.style.transform = 'translateY(0)';
        }
      });
    }, observerOptions);

    // Initialize animations on load
    window.addEventListener('DOMContentLoaded', () => {
      // Animate section title
      const sectionTitle = document.querySelector('.section-title-wrapper');
      if (sectionTitle) {
        sectionTitle.style.opacity = '0';
        sectionTitle.style.transform = 'translateY(50px)';
        sectionTitle.style.transition = 'all 1s cubic-bezier(0.4, 0, 0.2, 1)';
        fadeInObserver.observe(sectionTitle);
      }

      // Animate image with scale
      const imageContainer = document.querySelector('.premium-image-container');
      if (imageContainer) {
        imageContainer.style.opacity = '0';
        imageContainer.style.transform = 'scale(0.9) translateX(-50px)';
        imageContainer.style.transition = 'all 1.2s cubic-bezier(0.4, 0, 0.2, 1) 0.2s';
        fadeInObserver.observe(imageContainer);
      }

      // Animate menu items sequentially
      const menuRows = document.querySelectorAll('.menu-row');
      menuRows.forEach((row, index) => {
        row.style.opacity = '0';
        row.style.transform = 'translateX(30px)';
        row.style.transition = `all 0.5s cubic-bezier(0.4, 0, 0.2, 1) ${0.2 + index * 0.05}s`;
        fadeInObserver.observe(row);
      });

      // Parallax effect on scroll
      let ticking = false;
      window.addEventListener('scroll', () => {
        if (!ticking) {
          window.requestAnimationFrame(() => {
            const scrolled = window.pageYOffset;
            const parallaxElements = document.querySelectorAll('.parallax-element');
            parallaxElements.forEach((el, index) => {
              const speed = el.dataset.speed || 0.5;
              el.style.transform = `translateY(${scrolled * speed}px)`;
            });

            // Image parallax - subtle effect within bounds
            const images = document.querySelectorAll('.parallax-image');
            images.forEach(image => {
              const rect = image.getBoundingClientRect();
              const viewportHeight = window.innerHeight;
              const imageCenter = rect.top + rect.height / 2;
              const viewportCenter = viewportHeight / 2;
              const offset = (viewportCenter - imageCenter) * 0.05; // Very subtle
              image.style.transform = `translateY(${offset}px)`;
            });

            ticking = false;
          });
          ticking = true;
        }
      });

      // Magnetic cursor effect for menu items
      const menuTable = document.querySelector('.menu-table');
      if (menuTable) {
        const menuRows = document.querySelectorAll('.menu-row');
        menuRows.forEach(row => {
          row.addEventListener('mouseenter', (e) => {
            row.style.transform = 'translateX(0)';
          });
          
          row.addEventListener('mouseleave', () => {
            row.style.transform = 'translateX(0)';
          });
        });
      }

      // Glowing particles animation
      const sections = document.querySelectorAll('.section');
      sections.forEach(section => {
        if (section) {
          for (let i = 0; i < 15; i++) {
            const particle = document.createElement('div');
            particle.className = 'floating-particle';
            particle.style.cssText = `
              position: absolute;
              width: ${Math.random() * 4 + 2}px;
              height: ${Math.random() * 4 + 2}px;
              background: rgba(255, 184, 28, ${Math.random() * 0.5 + 0.3});
              border-radius: 50%;
              left: ${Math.random() * 100}%;
              top: ${Math.random() * 100}%;
              animation: float ${Math.random() * 10 + 10}s ease-in-out infinite;
              animation-delay: ${Math.random() * 5}s;
              pointer-events: none;
              box-shadow: 0 0 20px rgba(255, 184, 28, 0.5);
            `;
            section.appendChild(particle);
          }
        }
      });

      // Ripple effect on image hover
      const imageHolder = document.querySelector('.card-banner');
      if (imageHolder) {
        imageHolder.addEventListener('mouseenter', function(e) {
          this.style.transform = 'scale(1.05)';
          this.style.transition = 'transform 0.6s cubic-bezier(0.4, 0, 0.2, 1)';
        });
        
        imageHolder.addEventListener('mouseleave', function(e) {
          this.style.transform = 'scale(1)';
        });
      }

      // Price number counting animation
      const prices = document.querySelectorAll('.price-animate');
      prices.forEach(priceEl => {
        const target = parseInt(priceEl.textContent.match(/\d+/)[0]);
        let current = 0;
        const increment = target / 30;
        const timer = setInterval(() => {
          current += increment;
          if (current >= target) {
            current = target;
            clearInterval(timer);
          }
          priceEl.textContent = priceEl.textContent.replace(/\d+/, Math.floor(current));
        }, 30);
      });
    });

    // Add floating animation keyframes
    const style = document.createElement('style');
    style.textContent = `
      @keyframes float {
        0%, 100% {
          transform: translate(0, 0) rotate(0deg);
          opacity: 0.3;
        }
        25% {
          transform: translate(30px, -30px) rotate(90deg);
          opacity: 0.8;
        }
        50% {
          transform: translate(60px, 0px) rotate(180deg);
          opacity: 0.5;
        }
        75% {
          transform: translate(30px, 30px) rotate(270deg);
          opacity: 0.8;
        }
      }

      @keyframes shimmer {
        0% {
          background-position: -1000px 0;
        }
        100% {
          background-position: 1000px 0;
        }
      }

      @keyframes glow-pulse {
        0%, 100% {
          box-shadow: 0 0 10px rgba(255, 184, 28, 0.3);
        }
        50% {
          box-shadow: 0 0 30px rgba(255, 184, 28, 0.6);
        }
      }

      .menu-row {
        position: relative;
      }

      .corner-decoration {
        animation: glow-pulse 3s ease-in-out infinite;
      }
    `;
    document.head.appendChild(style);
  </script>

